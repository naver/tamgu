\ls12\ilvl1
\f9 \up0 \nosupersub \ulnone {\listtext	o	}
\f1 \up0 \nosupersub \ulnone %p\'a0 is any punctuations\
\ls12\ilvl1
\f9 \up0 \nosupersub \ulnone {\listtext	o	}
\f1 \up0 \nosupersub \ulnone %r\'a0 is a carriage return both \\n and \\r\
\ls12\ilvl1
\f9 \up0 \nosupersub \ulnone {\listtext	o	}
\f1 \up0 \nosupersub \ulnone %s\'a0 is a space (32) or a tab (09)\
\ls12\ilvl1
