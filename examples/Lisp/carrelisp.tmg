function carré(int x) {
    return x^^2;
}


function appelcarré(int x) {
    return \(+ x (carré x));
}

\(label tst ())

\(defun testfor (x y)
    (for x y
        (setq tst (cons x tst))
    )
    tst
)


ivector iv = [1..10];

println(\(testfor 0 iv));
